\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  font-weight: 700;\r\n  width: 100%;\r\n  height: 70vh;\r\n}\r\n.h2-parallax {\r\n  font-size: 60px;\r\n  background-color: rgba(238, 238, 238, 0.445);\r\n  padding: 8px 24px;\r\n  border-radius: 24px;\r\n}\r\n.div-parallax:nth-child(1) {\r\n  background-image: url(../img/book-and-cf.jpg);\r\n  background-attachment: fixed;\r\n  background-position: center;\r\n  background-size: cover;\r\n  overflow: hidden;\r\n}\r\n.div-parallax:nth-child(2) {\r\n  background-color: maroon;\r\n  background-position: center;\r\n  background-size: cover;\r\n}\r\n.div-parallax:nth-child(3) {\r\n  height: 400px;\r\n  background-image: url(../img/book-with-coffee.jpg);\r\n  background-attachment: fixed;\r\n  background-position: center;\r\n  background-size: cover;\r\n  overflow: hidden;\r\n}\r\n.div-parallax:nth-child(4) {\r\n  background-color: #2b2b52;\r\n}\r\n.footer {\r\n  background-color: #2b2b52;\r\n  color: #fff;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n.footer-div-1 {\r\n  display: flex;\r\n  justify-content: space-evenly;\r\n  border-bottom: 1px solid #000;\r\n  padding: 40px;\r\n}\r\n.footer-div-2 {\r\n  display: flex;\r\n  justify-content: center;\r\n  border-bottom: 1px solid #000;\r\n  font-size: 20px;\r\n  padding: 40px;\r\n}\r\n.footer-div-4 {\r\n  text-align: center;\r\n  padding: 30px;\r\n  font-size: 20px;\r\n}\r\n.footer-signup-btn,\r\n.footer-signup-btn:active,\r\n.footer-signup-btn:link {\r\n  background-color: red;\r\n  color: #fff;\r\n  text-decoration: none;\r\n  border-radius: 100px;\r\n  padding: 10px;\r\n}\r\n.footer-signup-btn:active:hover,\r\n.footer-signup-btn:hover,\r\n.footer-signup-btn:link:hover {\r\n  background-color: #00f;\r\n  color: #fff;\r\n}\r\n@media (max-width: 600px) {\r\n  .card-books {\r\n    display: block;\r\n    padding-left: 0;\r\n  }\r\n  .aos-img {\r\n    width: 300px;\r\n    margin-left: 10px;\r\n  }\r\n  .carousel-img {\r\n    background-size: cover;\r\n    height: 50vh;\r\n    opacity: 0.5;\r\n  }\r\n}\r\n@media (max-width: 500px) {\r\n  .dropdown-menu {\r\n    margin-left: 0;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"15c6f5a4a146001f7859fafa15fb352e","cacheData":{"env":{}}}